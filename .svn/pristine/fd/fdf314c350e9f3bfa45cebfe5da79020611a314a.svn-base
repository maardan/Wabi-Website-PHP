/*Credits: http://codepen.io/dimbslmh/full/mKfCc*/
.modal {
    text-align: center;
    padding: 0!important;
}

.modal:before {
    content: '';
    display: inline-block;
    height: 100%;
    vertical-align: middle;
    margin-right: -4px;
}

.modal-dialog {
    display: inline-block;
    text-align: left;
    vertical-align: middle;
}

.row-grid{
    margin-top: 30px;
}

.wabi-footer {
    padding: 30px 0 30px 0;
    background-color: #333;
    color: #9d9d9d;
    width: 100%;
}

.wabi-footer h3{
    text-transform: uppercase;
    font-size: 14px;
    font-weight: bold;
    color: white;
}

.wabi-footer ul{
    margin: 0;
    padding-left: 0;
    margin-top: 25px    
}

.wabi-footer li{
    font-size: 12px;
    font-weight: normal;
    list-style: none;    
}

.wabi-footer a{
    color: #9d9d9d;
}

.row-item-top{
    padding-top: 40px;
}

.wabi-page-header{
    padding-bottom: 9px;
    margin: 40px 0 0 0;
    border-bottom: 1px solid #eee;
}

.asset-title{
    text-align: center;
    text-transform: uppercase;
}

.center-text{
    text-align: center;
}

.dashboard-title{
    text-align: center;
    text-transform: uppercase;
}

.form-container {
    padding-top: 10px;
    padding-bottom: 10px;
    padding-left: 20px;
    padding-right: 20px;
}

/* Credit: http://www.bootply.com/qjvYXfWLuN */
.row.vdivide [class*='col-']:not(:last-child):after {
    background: #e0e0e0;
    width: 1px;
    content: "";
    display:block;
    position: absolute;
    top:0;
    bottom: 0;
    right: 0;
    min-height: 70px;
}

.form-inline{
    width: 100%;
}

.form-control-inline {
    min-width: 0;
    width: 100% !important; 
    display: inline;
}

.btn-width{
    width: 100%;
}

.btn-margin{
    margin-top: 20px;
    margin-bottom: 20px;
}

.wabi-footer-bottom{
    padding-top: 10px;
    background-color: #222;
    width: 100%;
}

.wabi-footer-bottom p{
    color: #9d9d9d;
    margin-left: 15px;
    font-size: 12px;
}

.wabi-row-margin-bottom {
    margin-bottom: 10px;
}

.shadow{
    box-shadow: rgba(0,0,0,0.5) 0px 0px 3px;
}


/* Asset.php */

#asset-display {
    text-align: center;
}

#asset-container {
