/*Landing Page Styling*/

.caption-small {
    height: 49px;
    overflow: hidden;
}

.caption-small h4 {
    white-space: nowrap;
}

.thumbnail img {
    width: 100%;
    padding: 9px;    
}

.thumbnail {
    padding: 0;
    border-radius:5px;
    border: 10px solid white;
    margin-bottom: 30px;
}

.thumbnail .caption-small {
    padding: 9px;
    color: #333;
}

.thumbnail-btns{
    padding-right: 10px;
    padding-left: 10px;
}

.lnd-container{
    margin-bottom: 100px;
}

.box{
    width: 100%;
    height: 200px;
    overflow: hidden;
}

.box-small{
    width: 100%;
    height: 150px;
    overflow: hidden;
}

.glyphicon-search{
    font-size: 12px;
}

.index-logo{
    font-family: "Raleway", sans-serif;
    font-weight: 900;
    background-color: transparent;
}

.navbar-logo{
    font-family: "Raleway", sans-serif;
    font-weight: 900;
    background-color: transparent;
}

.navbar-brand{
    font-size: 30px;
}

.navbar-inverse .navbar-brand{
    color: white;
}

.page-title{
    text-align: center;
    margin-bottom: 20px;
    font-weight: 700;
}

/* Thumbnails for rest of site*/
.caption {
    height: 110px;
    overflow: hidden;
}

.caption h4 {
    white-space: nowrap;
}

.thumbnail .caption {
    padding: 9px;
    color: #333;
}

/*Sticky Footer*/
*{
    margin: 0;
}

html, body{
    height: 100%;
}

.page-wrap{
    min-height: 100%;
    margin-bottom: -191px;
}

.page-wrap:after{
    content: "";
    display: block;
}
.sticky-footer, .page-wrap:after{
    height: 191px;    
}

.earnings-today {
    margin:20px auto;
}

.earnings-today p {
    text-align: center;
    font-size:45px;
    color:rgb(30,150,30);
}

.id-text{
    color: #afcee9;
}

/* Chartist */

.ct-series-a .ct-line {
  /* Set the colour of this series line */
  stroke: yellowgreen;
  /* Control the thikness of your lines */
  stroke-width: 2px;
}

.ct-series-a .ct-point {
  /* Colour of your points */
  stroke: forestgreen;
  /* Size of your points */
  stroke-width: 5px;
  /* Make your points appear as squares */
  stroke-linecap: circle;
}

.video-responsive{
    max-width: 100%;
    height: auto;
}

.thumbnail video{
    width: 100%;
    padding: 9px;
}

.watermark-thumb{
    position: absolute;
    top: 25%;
    left: 35%;
    opacity: 0.5;
    filter:alpha(opacity=50);
    color: white;
    font-size: 44px;
    font-weight: 900; 
    -webkit-touch-callout: none;
    -webkit-user-select: none;
    -khtml-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    -o-user-select: none;
    user-select: none;
}

.watermark-index{
    position: absolute;
    top: 25%;
