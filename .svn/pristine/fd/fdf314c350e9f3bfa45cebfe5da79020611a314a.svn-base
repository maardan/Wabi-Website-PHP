@import url(https://fonts.googleapis.com/css?family=Raleway:400,700,900);

body{
    padding-top: 70px; /* Required padding for .navbar-fixed-top. Remove if using .navbar-static-top. Change if height of navigation changes. */
    position: relative;
    background-color: #eff0f0;
}

.container-fluid .row .title {
    text-align: center;
    background-color:white;
    font-family:"Raleway", sans-serif;
    color:rgb(90,90,90);
}

.wabi-text {
    font-family:"Raleway", sans-serif;
    color: rgb(90,90,90);
    font-size: 20px;
    font-weight: bold;
}

.wabi-text-lg {
    font-family:"Raleway", sans-serif;
    color: rgb(90,90,90);
    font-size: 50px;
    font-weight: bold;
}

.wabi-text-sm {
    font-size: 15px;
    font-weight: normal;
}

.container .row #results-header {
    text-align: center;
    display:none;
    position:relative;
}

.navbar navbar-inverse {
    color:rgb(90,90,90);
}

.btn-float-right {
    float: right !important;
}

.panel-empty{
    padding-left:15px;
    padding-right:15px;
    padding-top:0px;
    padding-bottom:0px;
    border:0px;
    -webkit-box-shadow: 0 ;
    box-shadow: 0 ;
}

.s-link{
    display: inline;
    padding-right: 20px;
}

.btn-align{
    display: inline;
    float: right;
}

.portfolio-item {
    margin-bottom: 25px;
}

#wabi-login-form {
    width: 50%; 
    margin: 0 auto; 
}

.header-image {
    display: block;
    width: 100%;
    text-align: center;
    -webkit-background-size: cover;
    -moz-background-size: cover;
    background-size: cover;
    -o-background-size: cover;
}

.headline {
    padding: 1% 0;
}

.headline h1 {
    font-size: 130px;
}

.headline h2 {
    font-size: 77px;
}


@media(max-width:1200px) {
    .headline h1 {
        font-size: 140px;
    }

    .headline h2 {
        font-size: 63px;
    }
    
    body{
        padding-top: 100px;
    }


}

@media(max-width:991px) {
    .headline h1 {
        font-size: 105px;
    }

    .headline h2 {
        font-size: 50px;
    }
    
}

@media(max-width:768px) {
    .container {
        margin: 0 15px;
    }

}

@media(max-width:668px) {
    .headline h1 {
        font-size: 70px;
    }

    .headline h2 {
        font-size: 42px;
    }

}

@media(max-width:640px) {

    .headline h1 {
        font-size: 60px;
    }

    .headline h2 {
        font-size: 40px;
    }
}

@media(max-width:375px) {
}

